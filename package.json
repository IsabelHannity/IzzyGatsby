    "up": "docker-compose -f docker/docker-compose.yaml up -d --build",
    "down": "docker-compose -f docker/docker-compose.yaml down"
  },
  "dependencies": {
    "gatsby": "^3.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "gatsby-plugin-postcss": "^4.0.0",
    "postcss": "^8.2.8",
    "tailwindcss": "^2.0.3"
  }
}
