{
  "name": "IzzyGatsby",
  "version": "1.0.0",
  "private": true,
  "description": "IzzyGatsby",
  "author": "Matthew & Izzy",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
